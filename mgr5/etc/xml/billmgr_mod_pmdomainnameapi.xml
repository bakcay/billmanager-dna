<?xml version="1.0" encoding="UTF-8"?>
<mgrdata>
  <plugin name="pmdomainnameapi">
    <group>processing_module</group>
    <params>
      <type name="domain"/>
    </params>
    <msg name="desc_short" lang="ru">DomainNameAPI Регистратор</msg>
    <msg name="desc_short" lang="en">DomainNameAPI Registrar</msg>
    <msg name="desc_full" lang="ru">Модуль регистрации доменов через DomainNameAPI</msg>
    <msg name="desc_full" lang="en">Domain registration module via DomainNameAPI</msg>
    <msg name="price" lang="ru">Бесплатно</msg>
    <msg name="price" lang="en">Free</msg>
  </plugin>



  <metadata name="processing.edit.pmdomainnameapi" type="form">
    <form title="name">
      <page name="connect">
        <field name="registrar_name">
          <input name="registrar_name" required="yes" type="text"/>
        </field>
        <field name="dna_username">
          <input name="dna_username" required="yes" type="text"/>
        </field>
        <field name="dna_password">
          <input name="dna_password" required="yes" type="password" crypted="yes"/>
        </field>
        <field name="test_mode">
          <input name="test_mode" type="checkbox"/>
        </field>
      </page>
    </form>
  </metadata>

  <lang name="en">
    <messages name="label_processing_modules">
      <msg name="pmdomainnameapi">DomainNameAPI Registrar</msg>
      <msg name="module_pmdomainnameapi">DomainNameAPI Registrar</msg>
    </messages>
    <messages name="processing.edit.pmdomainnameapi">
      <msg name="registrar_name">Name of Registrar</msg>
      <msg name="hint_registrar_name">Name of registrar for WHOIS</msg>
      <msg name="dna_username">DomainNameAPI Username</msg>
      <msg name="hint_dna_username">Your DomainNameAPI account username</msg>
      <msg name="dna_password">DomainNameAPI Password</msg>
      <msg name="hint_dna_password">Your DomainNameAPI account password</msg>
      <msg name="test_mode">Test Mode</msg>
      <msg name="hint_test_mode">Enable test mode for development</msg>
      <!-- Validation messages -->
      <msg name="error_dna_username_required">DomainNameAPI username is required</msg>
      <msg name="error_dna_username_length">Username must be between 3 and 64 characters</msg>
      <msg name="error_dna_password_required">DomainNameAPI password is required</msg>
      <msg name="error_dna_password_length">Password must be between 6 and 64 characters</msg>
    </messages>
  </lang>

  <lang name="ru">
    <messages name="label_processing_modules">
      <msg name="pmdomainnameapi">DomainNameAPI Регистратор</msg>
      <msg name="module_pmdomainnameapi">DomainNameAPI Регистратор</msg>
    </messages>
    <messages name="processing.edit.pmdomainnameapi">
      <msg name="registrar_name">Название регистратора</msg>
      <msg name="hint_registrar_name">Название регистратора для WHOIS</msg>
      <msg name="dna_username">Имя пользователя DomainNameAPI</msg>
      <msg name="hint_dna_username">Ваше имя пользователя DomainNameAPI</msg>
      <msg name="dna_password">Пароль DomainNameAPI</msg>
      <msg name="hint_dna_password">Ваш пароль DomainNameAPI</msg>
      <msg name="test_mode">Тестовый режим</msg>
      <msg name="hint_test_mode">Включить тестовый режим для разработки</msg>
      <!-- Validation messages -->
      <msg name="error_dna_username_required">Требуется имя пользователя DomainNameAPI</msg>
      <msg name="error_dna_username_length">Имя пользователя должно быть от 3 до 64 символов</msg>
      <msg name="error_dna_password_required">Требуется пароль DomainNameAPI</msg>
      <msg name="error_dna_password_length">Пароль должен быть от 6 до 64 символов</msg>
    </messages>
  </lang>

  <lang name="tr">
    <messages name="label_processing_modules">
      <msg name="pmdomainnameapi">DomainNameAPI Alan Adı Sağlayıcı</msg>
