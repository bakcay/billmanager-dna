      <msg name="error_dna_username_length">Kullanıcı adı 3-64 karakter arasında olmalı</msg>
      <msg name="error_dna_password_required">DomainNameAPI şifre gerekli</msg>
      <msg name="error_dna_password_length">Şifre 6-64 karakter arasında olmalı</msg>
    </messages>
  </lang>

  <lang name="de">
    <messages name="label_processing_modules">
      <msg name="pmdomainnameapi">DomainNameAPI Registrar</msg>
