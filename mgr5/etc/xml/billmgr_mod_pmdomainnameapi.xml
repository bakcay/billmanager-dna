      <msg name="hint_dna_username">Ihr DomainNameAPI Benutzername</msg>
      <msg name="dna_password">DomainNameAPI Passwort</msg>
      <msg name="hint_dna_password">Ihr DomainNameAPI Passwort</msg>
      <msg name="test_mode">Testmodus</msg>
      <msg name="hint_test_mode">Testmodus für Entwicklung aktivieren</msg>
      <!-- Validierungsmeldungen -->
      <msg name="error_dna_username_required">DomainNameAPI Benutzername ist erforderlich</msg>
      <msg name="error_dna_username_length">Benutzername muss zwischen 3 und 64 Zeichen lang sein</msg>
      <msg name="error_dna_password_required">DomainNameAPI Passwort ist erforderlich</msg>
      <msg name="error_dna_password_length">Passwort muss zwischen 6 und 64 Zeichen lang sein</msg>
    </messages>
  </lang>

  <lang name="nl">
    <messages name="label_processing_modules">
      <msg name="pmdomainnameapi">DomainNameAPI Registrar</msg>
      <msg name="module_pmdomainnameapi">DomainNameAPI Registrar</msg>
    </messages>
    <messages name="processing.edit.pmdomainnameapi">
      <msg name="registrar_name">Naam van Registrar</msg>
      <msg name="hint_registrar_name">Naam van registrar voor WHOIS</msg>
      <msg name="dna_username">DomainNameAPI Gebruikersnaam</msg>
      <msg name="hint_dna_username">Uw DomainNameAPI gebruikersnaam</msg>
      <msg name="dna_password">DomainNameAPI Wachtwoord</msg>
      <msg name="hint_dna_password">Uw DomainNameAPI wachtwoord</msg>
      <msg name="test_mode">Testmodus</msg>
      <msg name="hint_test_mode">Testmodus inschakelen voor ontwikkeling</msg>
      <!-- Validatie berichten -->
      <msg name="error_dna_username_required">DomainNameAPI gebruikersnaam is vereist</msg>
      <msg name="error_dna_username_length">Gebruikersnaam moet tussen 3 en 64 tekens zijn</msg>
      <msg name="error_dna_password_required">DomainNameAPI wachtwoord is vereist</msg>
